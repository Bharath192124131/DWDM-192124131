# Data
speed_car <- c(78.3, 81.8, 82, 74.2, 83.4, 84.5, 82.9, 77.5, 80.9, 70.6)

# Calculate Interquartile Range (IQR)
interquartile_range <- IQR(speed_car)

# Calculate Standard Deviation
standard_deviation <- sd(speed_car)

cat("Interquartile Range (IQR):", interquartile_range, "\n")
cat("Standard Deviation:", standard_deviation, "\n")
